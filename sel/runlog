sel-scraper-1  | 	font-size: 14px;
sel-scraper-1  | }
sel-scraper-1  | 
sel-scraper-1  | @media ( max-width : 979px) {
sel-scraper-1  | 	.border-left {
sel-scraper-1  | 		border-left: 1px solid #dcddde;
sel-scraper-1  | 	}
sel-scraper-1  | }
sel-scraper-1  | </style>
sel-scraper-1  | 
sel-scraper-1  | <script>
sel-scraper-1  | 	var hasNext = true;
sel-scraper-1  | 	var listingTemplate = Handlebars.compile($("#listing-Template").html());
sel-scraper-1  | 	var checkHasNext = true;
sel-scraper-1  | 	
sel-scraper-1  | 	var statContainer = "<div class=\"box-r-footer\">$content</div>";
sel-scraper-1  | 	var organizationStat = '<div class="more-days inline $className"><h4 class="m-0">$number</h4><span class="em span12">$label</span></div>';
sel-scraper-1  | 	Handlebars.registerHelper('getStat', function(numOfCamps , numOfVols, options) {
sel-scraper-1  | 		function getStat(className, label, number) {
sel-scraper-1  | 			var htmlElement = organizationStat;
sel-scraper-1  | 			htmlElement = htmlElement.replace('$className' , className);
sel-scraper-1  | 			htmlElement = htmlElement.replace('$label' , label);
sel-scraper-1  | 			return htmlElement.replace('$number' , number);
sel-scraper-1  | 		}
sel-scraper-1  | 
sel-scraper-1  | 	  	if (!$.isNumeric(numOfCamps))
sel-scraper-1  | 	  		numOfCamps = parseInt(numOfCamps.replace ( /[^\d.]/g, '' ),10);
sel-scraper-1  | 
sel-scraper-1  | 	  	if (!$.isNumeric(numOfVols))
sel-scraper-1  | 	  		numOfVols = parseInt(numOfVols.replace ( /[^\d.]/g, '' ),10);
sel-scraper-1  | 
sel-scraper-1  | 	  	var campaignLabel = numOfCamps < 2 ? "campaign" : "campaigns";
sel-scraper-1  | 		var volunteerLabel = numOfVols < 2 ? "volunteer opportunity" : "volunteer opportunities";
sel-scraper-1  | 	  	if ( numOfCamps > 0 && numOfVols > 0) {
sel-scraper-1  | 	  		var campStat = getStat('span6', campaignLabel, numOfCamps);
sel-scraper-1  | 	  		var volStat = getStat('span6 border-left', volunteerLabel, numOfVols);
sel-scraper-1  | 	  		return new Handlebars.SafeString(statContainer.replace('$content' , campStat + volStat));
sel-scraper-1  | 	  	} else if (numOfCamps > 0) {
sel-scraper-1  | 	  		var campStat = getStat('span12', campaignLabel, numOfCamps);
sel-scraper-1  | 	  		return new Handlebars.SafeString(statContainer.replace('$content' , campStat));
sel-scraper-1  | 	  	} else if (numOfVols > 0) {
sel-scraper-1  | 	  		var volStat = getStat('span12', volunteerLabel, numOfVols);
sel-scraper-1  | 	  		return new Handlebars.SafeString(statContainer.replace('$content' , volStat));
sel-scraper-1  | 	  	}
sel-scraper-1  | 
sel-scraper-1  | 		return new Handlebars.SafeString('');
sel-scraper-1  | 	});
sel-scraper-1  | 	Handlebars.registerHelper('hasCampaignOrVolunteer', function(hasVolunteers , hasCampaigns) {
sel-scraper-1  | 			if(!(hasVolunteers || hasCampaigns)){
sel-scraper-1  | 				return new Handlebars.SafeString("gradient-over-image--clear");
sel-scraper-1  | 			}
sel-scraper-1  | 			return ;
sel-scraper-1  | 	});
sel-scraper-1  | 	
sel-scraper-1  | 	Handlebars.registerHelper('displayRaisedByAmt', function(campaignRaisedByAmt) {
sel-scraper-1  | 		if(campaignRaisedByAmt > 1){
sel-scraper-1  | 				return new Handlebars.SafeString('raised from '+campaignRaisedByAmt+' donors');
sel-scraper-1  | 		}else if(campaignRaisedByAmt < 1){
sel-scraper-1  | 			return new Handlebars.SafeString('');
sel-scraper-1  | 		}
sel-scraper-1  | 		return new Handlebars.SafeString('raised from '+campaignRaisedByAmt+' donor');
sel-scraper-1  | 	});
sel-scraper-1  | 	
sel-scraper-1  | 	Handlebars.registerHelper('checkPublicAccess', function(hasPublicAccess) {
sel-scraper-1  | 		return (!hasPublicAccess);
sel-scraper-1  | 	});
sel-scraper-1  | 	
