sel-scraper-1  | 
sel-scraper-1  | if ($('.nav-acdr').length) {
sel-scraper-1  | 	  $('.nav-acdr__head').click(function() {
sel-scraper-1  | 	    $(this).parent().toggleClass('is-expanded');
sel-scraper-1  | 	  })
sel-scraper-1  | 	  $('.nav-acdr .radio-filters__radio').change(function() {
sel-scraper-1  | 	    var title= $(this).siblings('.radio-filters__text-left').text();
sel-scraper-1  | 	    var stats= $(this).siblings('.radio-filters__text-right').text();
sel-scraper-1  | 	    var wrapper = $(this).closest('.nav-acdr');
sel-scraper-1  | 	    wrapper.removeClass('is-expanded');
sel-scraper-1  | 	    wrapper.find('.nav-acdr__title').text(title);
sel-scraper-1  | 	    wrapper.find('.nav-acdr__stats').text(stats);
sel-scraper-1  | 	  })
sel-scraper-1  | 	}
sel-scraper-1  | 
sel-scraper-1  | if ($("[data-role=list-show-more]").length) {
sel-scraper-1  | 	$("[data-role=list-show-more]").each(
sel-scraper-1  | 		function(index, ctn) {
sel-scraper-1  | 			// first we see if children height is larger than ctn's max height
sel-scraper-1  | 
sel-scraper-1  | 			// getting ctn height
sel-scraper-1  | 			$(ctn).css('max-height', 'none');
sel-scraper-1  | 			var ctnHeight = $(ctn).outerHeight();
sel-scraper-1  | 
sel-scraper-1  | 			$(ctn).children(".title-ctn__body").css('max-height', '');
sel-scraper-1  | 
sel-scraper-1  | 			// calculate total children height
sel-scraper-1  | 			var bodyChildrenHeight = (function() {
sel-scraper-1  | 				var totalHeight = 0;
sel-scraper-1  | 				$(ctn).children(".title-ctn__body")
sel-scraper-1  | 					.find('.title-ctn__child')
sel-scraper-1  | 					.each(function(index, child) {
sel-scraper-1  | 						totalHeight += $(child).outerHeight(true);
sel-scraper-1  | 					});
sel-scraper-1  | 				return totalHeight;
sel-scraper-1  | 			}());
sel-scraper-1  | 
sel-scraper-1  | 			var offset = ctnHeight - bodyChildrenHeight;
sel-scraper-1  | 
sel-scraper-1  | 			var bodyMaxHeight = parseInt($(ctn).children(".title-ctn__body")
sel-scraper-1  | 				.css('max-height'), 10);
sel-scraper-1  | 			// console.log(bodyChildrenHeight,bodyMaxHeight)
sel-scraper-1  | 			if (bodyChildrenHeight > bodyMaxHeight) {
sel-scraper-1  | 				// if children height exceeded max height of ctn...
sel-scraper-1  | 
sel-scraper-1  | 				// shows the expand button
sel-scraper-1  | 				$(ctn).children('.title-ctn__expand').show();
sel-scraper-1  | 
sel-scraper-1  | 				// upon clicking the expand button
sel-scraper-1  | 				$(ctn).children('.title-ctn__expand').click(
sel-scraper-1  | 						function() {
sel-scraper-1  | 							// max height of ctn is increased
sel-scraper-1  | 							$(this).siblings('.title-ctn__body')
sel-scraper-1  | 								.addClass('title-ctn__body--long');
sel-scraper-1  | 							// children is unhidden
sel-scraper-1  | 							$(this).siblings('.title-ctn__body').find('.title-ctn__child').show();
sel-scraper-1  | 							$(this).hide();
sel-scraper-1  | 							$(this).siblings('.title-ctn__collapse').show();
sel-scraper-1  | 						}
sel-scraper-1  | 					)
sel-scraper-1  | 					// upon clicking the collapse button
sel-scraper-1  | 				$(ctn).children('.title-ctn__collapse').click(
sel-scraper-1  | 					function() {
sel-scraper-1  | 						// max height of ctn is increased
sel-scraper-1  | 						$(this).siblings('.title-ctn__body')
sel-scraper-1  | 							.removeClass('title-ctn__body--long');
sel-scraper-1  | 						// hide back children
sel-scraper-1  | 						hideOverflowChildren();
sel-scraper-1  | 						$(this).hide();
sel-scraper-1  | 						$(this).siblings('.title-ctn__expand').show();
sel-scraper-1  | 					}
sel-scraper-1  | 				)
sel-scraper-1  | 
sel-scraper-1  | 				hideOverflowChildren();
sel-scraper-1  | 
sel-scraper-1  | 				function hideOverflowChildren() {
sel-scraper-1  | 					var runningChildHeight = 0;
sel-scraper-1  | 					var heightExceeded = false;
sel-scraper-1  | 					$(ctn).children(".title-ctn__body")
sel-scraper-1  | 						.find('.title-ctn__child')
sel-scraper-1  | 						.each(function(index, child) {
sel-scraper-1  | 							if (!heightExceeded) {
sel-scraper-1  | 								var childHeight = $(child).outerHeight(true);
sel-scraper-1  | 								runningChildHeight += childHeight;
sel-scraper-1  | 								// console.log(runningChildHeight, bodyMaxHeight-offset)
sel-scraper-1  | 								if (runningChildHeight > bodyMaxHeight - offset) {
sel-scraper-1  | 									$(child).delay(300).hide(0);
sel-scraper-1  | 								}
sel-scraper-1  | 
sel-scraper-1  | 							} else {
sel-scraper-1  | 								$(child).delay(300).hide(0);
sel-scraper-1  | 							}
sel-scraper-1  | 						})
sel-scraper-1  | 				}
sel-scraper-1  | 			}
sel-scraper-1  | 
sel-scraper-1  | 		}
sel-scraper-1  | 	);
sel-scraper-1  | }
sel-scraper-1  | 
sel-scraper-1  | 
sel-scraper-1  | function triggerSearch() {
sel-scraper-1  | 	$('.nav__body-search-trigger-open').click(function(event) {
sel-scraper-1  | 		event.preventDefault();
sel-scraper-1  | 		$('.nav__body-search').show();
sel-scraper-1  | 		$('.nav__body-search-form-input').focus();
sel-scraper-1  | 		$('.nav__body-mobile-menu, .nav__body-logo, .nav__body-links, .nav__body-dock').hide();
sel-scraper-1  | 	});
sel-scraper-1  | 
sel-scraper-1  | 	$('.nav__body-search-trigger-close').click(function(event) {
sel-scraper-1  | 		event.preventDefault();
sel-scraper-1  | 
sel-scraper-1  | 		$('.nav__body-search').hide();
sel-scraper-1  | 		$('.nav__body-mobile-menu, .nav__body-logo, .nav__body-links, .nav__body-dock').show();
sel-scraper-1  | 	});
sel-scraper-1  | }
