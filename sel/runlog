sel-scraper-1  | 			        panel.removeClass('is-expanded');
sel-scraper-1  | 			    })
sel-scraper-1  | 			    $(this).addClass('is-active');
sel-scraper-1  | 			    targetPanel.toggleClass('is-expanded');
sel-scraper-1  | 			  });
