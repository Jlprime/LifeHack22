sel-scraper-1  | 		 $('[data-role~="reveal-toggle"]').click(function(e) {
sel-scraper-1  | 			  e.preventDefault();
sel-scraper-1  | 			  var group= $(this).data('group');
sel-scraper-1  | 			  var target= $(this).data('target').split(' ');
sel-scraper-1  | 				
sel-scraper-1  | 			  var $target= function() {
sel-scraper-1  | 			    var selector='';
sel-scraper-1  | 			    target.forEach(function(currentVal,index) {
sel-scraper-1  | 			      if (index!=0) {
sel-scraper-1  | 			        selector+=',';
sel-scraper-1  | 			      }
sel-scraper-1  | 			      selector+= '[data-tag~='+currentVal+']';
sel-scraper-1  | 			    })
sel-scraper-1  | 			    return $(selector);
sel-scraper-1  | 			  }();
sel-scraper-1  | 			  
sel-scraper-1  | 			  	$('[data-group~='+ group +'][data-role~="reveal-toggle"]').removeClass('is-active');
sel-scraper-1  | 			  	$(this).addClass('is-active');
sel-scraper-1  | 			  	$('[data-group~='+ group +'][data-role~="reveal-content"]').removeClass('is-visible');
sel-scraper-1  | 			 	
sel-scraper-1  | 			  if ($target.length) {
sel-scraper-1  | 			    $target.addClass('is-visible');
sel-scraper-1  | 			  }
sel-scraper-1  | 			  $('.search-result__body').find('.distance_town_tab').removeClass('is-visible');
sel-scraper-1  | 			  
sel-scraper-1  | 			  // NVPCGSG-1236
sel-scraper-1  | 			  console.log("target: " + target);
sel-scraper-1  | 			  if (target == "donate" || target == "volunteer") {
sel-scraper-1  | 				$('.search-result__body').find('.causes_div').removeClass('mb-40');
sel-scraper-1  | 			  } else {
sel-scraper-1  | 				$('.search-result__body').find('.causes_div').addClass('mb-40');
sel-scraper-1  | 			  }
sel-scraper-1  | 			});
sel-scraper-1  | 		 function triggerTabsRevealContent (group , target){
