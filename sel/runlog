sel-scraper-1  | 			        $self.children('.radio-filters__radio').removeAttr('checked');
sel-scraper-1  | 			        $self.children('.radio-filters__radio').trigger('deselect');
sel-scraper-1  | 			    },0);
sel-scraper-1  | 
sel-scraper-1  | 			    };
sel-scraper-1  | 			    var unbind = function() {
sel-scraper-1  | 			      $self.unbind('mouseup',up);
sel-scraper-1  | 			    };
sel-scraper-1  | 			    var up = function() {
sel-scraper-1  | 			      uncheck();
sel-scraper-1  | 			      unbind();
sel-scraper-1  | 			    };
sel-scraper-1  | 			    $self.bind('mouseup',up);
sel-scraper-1  | 
sel-scraper-1  | 			    //this code is not necessary for deselection but it helps us to detect deselection
sel-scraper-1  | 
sel-scraper-1  | 			    $self.one('mouseout', unbind);
sel-scraper-1  | 			  }
sel-scraper-1  | 			});
