sel-scraper-1  | <script>
sel-scraper-1  | Liferay.on('updateGivingCart',function(data) {
sel-scraper-1  | 	function getTotalAmt(totalAmt){
sel-scraper-1  | 		return getAmountWithCommas(totalAmt);
sel-scraper-1  | 	}
sel-scraper-1  | 	
sel-scraper-1  | 	var donationCount = 0;
sel-scraper-1  | 	var donationListing = data.cartList;
sel-scraper-1  | 	var givingCartListing = '';
sel-scraper-1  | 	
sel-scraper-1  | 	if (donationListing) {
sel-scraper-1  | 		var totalAmt = 0;
sel-scraper-1  | 		var donationCount = 0;
sel-scraper-1  | 		var donationVisible = 3; 
sel-scraper-1  | 	    $.each(donationListing, function(index) {
sel-scraper-1  | 	        var item = donationListing[index].receiverName;
sel-scraper-1  | 	        var amt = donationListing[index].donationAmount;
sel-scraper-1  | 	        var isExpired = donationListing[index].expired == "expired" ? true : false;
sel-scraper-1  | 	        
sel-scraper-1  | 	        if (!isExpired) {
sel-scraper-1  | 	        	donationCount++;
sel-scraper-1  | 		        totalAmt += amt;
sel-scraper-1  | 		        if(donationCount <= donationVisible)
sel-scraper-1  | 		        	givingCartListing += 
sel-scraper-1  | 		        		'<li>' + '<span class="uf-dockbar-name">' + item + '</span>' + 
sel-scraper-1  | 		        			'<div class="uf-dockbar-price">' + getTotalAmt(amt) +'</div>' + 
sel-scraper-1  | 		        			'<a href="javascript:;" class="uf-dockbar-item-remove uf-dockbar-img-replace">Remove</a>' +
sel-scraper-1  | 		        		'</li>' ;
sel-scraper-1  | 	        }
sel-scraper-1  | 	    });
sel-scraper-1  | 		var donationHaveMoreItems = donationCount - donationVisible; 
sel-scraper-1  | 	    
sel-scraper-1  | 	    totalAmt = getTotalAmt(totalAmt);
sel-scraper-1  | 	    
sel-scraper-1  | 	    $('ul.uf-dockbar-cart-items li:not(li.uf-dockbar-more-items-message)').remove();
sel-scraper-1  | 	    $('ul.uf-dockbar-cart-items li.uf-dockbar-more-items-message').before(givingCartListing);
sel-scraper-1  | 	    $('div.uf-dockbar-cart-total span>span, span.uf-dockbar-cart-label').text(totalAmt);
sel-scraper-1  | 	    if (donationCount > 0) {
sel-scraper-1  | 			$('.checkout-btn').removeAttr('disabled');
sel-scraper-1  | 			$('.checkout-btn').removeClass('button-disabled');
sel-scraper-1  | 			$('li.uf-dockbar-more-items-message').html('<b>' + donationHaveMoreItems + '</b> more item' + (donationHaveMoreItems > 1 ? 's' : '') + '<h5>We love what you\'ve added to your basket today.</h5>');
sel-scraper-1  | 		} else {
sel-scraper-1  | 			$('.checkout-btn').attr('disabled', 'disabled');
sel-scraper-1  | 			$('.checkout-btn').addClass('button-disabled');
sel-scraper-1  | 			$('li.uf-dockbar-more-items-message').html('Fill it with purpose! Did you mean to add something here?');
sel-scraper-1  | 		}
sel-scraper-1  | 
sel-scraper-1  | 		if (donationCount > donationVisible || donationCount == 0 ) {
sel-scraper-1  | 			$('li.uf-dockbar-more-items-message').removeClass('hide');
sel-scraper-1  | 		} else {
sel-scraper-1  | 			$('li.uf-dockbar-more-items-message').addClass('hide');
sel-scraper-1  | 		}
sel-scraper-1  | 	}
sel-scraper-1  | });
sel-scraper-1  | </script>
sel-scraper-1  | <!-- LOAD CART ITEM ON PAGE LOAD -->
