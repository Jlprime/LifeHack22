sel-scraper-1  | 	function isInArray(array , s) {
sel-scraper-1  | 		for (i = 0 ; i < array.length ; i ++) {
sel-scraper-1  | 			if (array[i] == s) {
sel-scraper-1  | 				return true ;
sel-scraper-1  | 			}
sel-scraper-1  | 		}
sel-scraper-1  | 		return false ;
sel-scraper-1  | 	}
