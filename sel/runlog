sel-scraper-1  | function stickyMenu() {
sel-scraper-1  | 	var menu = document.querySelector('.nav__body');
sel-scraper-1  | 	if (!menu) return;
sel-scraper-1  | 	var menuPosition = menu.getBoundingClientRect().top;
sel-scraper-1  | 
sel-scraper-1  | 	window.addEventListener('scroll', function() {
sel-scraper-1  | 	// console.log(menuPosition);
sel-scraper-1  | 
sel-scraper-1  | 		if (window.pageYOffset > 0) {
sel-scraper-1  | //            $('.nav__body').addClass("fix-nav");
sel-scraper-1  | 			$('.nav').addClass("fix-nav");
sel-scraper-1  | 			$('.nav__body-phantom').show();
sel-scraper-1  | 		} else {
sel-scraper-1  | //            $('.nav__body').removeClass("fix-nav");
sel-scraper-1  | 			$('.nav').removeClass("fix-nav");
sel-scraper-1  | 			$('.nav__body-phantom').hide();
sel-scraper-1  | 		}
sel-scraper-1  | 	});
sel-scraper-1  | }
