sel-scraper-1  | 	//flag used to allow other UX behaviour to continue without triggering ajax call to search until all presets are properly set into form fields
sel-scraper-1  | 	var suppressFireSearch = true;
sel-scraper-1  | 	function fireSearchAjax(data, template, overwritten , count , main , updateCounter ) {
sel-scraper-1  | 		if (!suppressFireSearch) {
sel-scraper-1  | 			var loadMoreTemplate = Handlebars.compile($("#loadMore-template").html());
sel-scraper-1  | 			var loadMore = $('#loadMore');
sel-scraper-1  | 			searchListingHolder = $('#searchlisting');
sel-scraper-1  | 			searchResultHolder = $('#search-result-holder');
sel-scraper-1  | 			var checkScroll = 0;
sel-scraper-1  | 			var loadmore = loadMoreTemplate();
sel-scraper-1  | 			if (overwritten) {
sel-scraper-1  | 				searchListingHolder.empty().html(loadmore);
sel-scraper-1  | 			}
sel-scraper-1  | 			var data2 = JSON.stringify(data) ; 
sel-scraper-1  | 			$('#lastUrl').val(JSON.stringify(data));
sel-scraper-1  | 
sel-scraper-1  | 			// Set keyword
sel-scraper-1  | 			var q = $('#search_term').val();
sel-scraper-1  | 			if(q == null || q== undefined ){
sel-scraper-1  | 				q = ''; 
sel-scraper-1  | 			}else{
sel-scraper-1  | 				q = q.trim();
sel-scraper-1  | 				if(q.length == 0){
sel-scraper-1  | 					q = '';
sel-scraper-1  | 				}
sel-scraper-1  | 			}
sel-scraper-1  | 			$('#lastKeyword').val(q);
sel-scraper-1  | 			if(main){
sel-scraper-1  | 				$('#loadingGif').show();
sel-scraper-1  | 			}
sel-scraper-1  | 						
sel-scraper-1  | 			var positionBeforeLoad = $(window).scrollTop();
sel-scraper-1  | 	        
sel-scraper-1  | 			$.ajax({
sel-scraper-1  | 				type : "POST",
sel-scraper-1  | 				dataType : "json",
sel-scraper-1  | 				url : 'https://www.giving.sg/search?p_p_id=search_WAR_givingsgportlet&p_p_lifecycle=2&p_p_state=normal&p_p_mode=view&p_p_resource_id=search&p_p_cacheability=cacheLevelPage&p_p_col_id=column-1&p_p_col_count=1',
sel-scraper-1  | 				data : '_search_WAR_givingsgportlet_data='+JSON.stringify(data)+'&'+"p_auth="+Liferay.authToken+'&'+"_search_WAR_givingsgportlet_count="+count+'&'+"_search_WAR_givingsgportlet_q="+encodeURIComponent(q),
sel-scraper-1  | 				complete : function(message){
sel-scraper-1  | 					if(main){
sel-scraper-1  | 						$('#loadingGif').hide();
sel-scraper-1  | 					}
sel-scraper-1  | 				},
sel-scraper-1  | 				success : function(message) {
sel-scraper-1  | 					if (!$.isEmptyObject(message)) {
sel-scraper-1  | 						var html = template(message);
sel-scraper-1  | 						//Truncate only items returned by query
sel-scraper-1  | 						checkHasNext = false;
sel-scraper-1  | 						hasNext = message.hasNext;
sel-scraper-1  | 						var requestData = message.requestData ; 
sel-scraper-1  | 						var q = message.q ; 
sel-scraper-1  | 						if(requestData == $('#lastUrl').val() && q == $('#lastKeyword').val()) {
sel-scraper-1  | 								
sel-scraper-1  | 							if (overwritten) {
sel-scraper-1  | 								searchListingHolder.empty().html(loadmore);
sel-scraper-1  | 							}
sel-scraper-1  | 								
sel-scraper-1  | 							//Truncate only items returned by query
sel-scraper-1  | 					        var $html = $("<div>" + html + "</div>");
sel-scraper-1  | 							$html.find('.truncate').succinct({size: 45});
sel-scraper-1  | 					    	$html.find('.truncate2').succinct({size: 125});
sel-scraper-1  | 							html = $html.html();
sel-scraper-1  | 							$(html).insertBefore("#loadMore");
sel-scraper-1  | 							searchListingHolder.children().show();
sel-scraper-1  | 								
sel-scraper-1  | 							if($(window).scrollTop() + $(window).height() > $(document).height() - 100) {
sel-scraper-1  | 							    // Scroll back to the position before loading
sel-scraper-1  | 									$("html, body").scrollTop(positionBeforeLoad);
sel-scraper-1  | 							}
sel-scraper-1  | 								
sel-scraper-1  | 							if(parseInt(message['allCount']) > 1 ){
sel-scraper-1  | 								$('#result_text').text("RESULTS FOUND");
sel-scraper-1  | 								searchResultHolder.find('strong').text(message['allCount'] + " ");
sel-scraper-1  | 							}else if(parseInt(message['allCount']) == 1 ){
sel-scraper-1  | 								$('#result_text').text("RESULT FOUND");
sel-scraper-1  | 								searchResultHolder.find('strong').text(message['allCount'] + " ");
sel-scraper-1  | 							}else {
sel-scraper-1  | 								$('#result_text').text("NO RESULTS FOUND");
sel-scraper-1  | 								searchResultHolder.find('strong').text("");
sel-scraper-1  | 							}
sel-scraper-1  | 								
sel-scraper-1  | 							if(updateCounter){
sel-scraper-1  | 								$('#charityAmount').text(message['charitiesCount']);
sel-scraper-1  | 								$('#campaignAmount').text(message['campaignCount']);
sel-scraper-1  | 								$('#adHocVolAmount').text(message['adhocCount']);
sel-scraper-1  | 								$('#regVolAmount').text(message['regularCount']);
sel-scraper-1  | 								$('#gumAmount').text(message['gumCount']);
sel-scraper-1  | 								$('#corporatesAmount').text(message['corporatesCount']);
sel-scraper-1  | 								if (main) {
sel-scraper-1  | 									$('#allAmount').text(parseInt(message['charitiesCount'])+parseInt(message['campaignCount'])
sel-scraper-1  | 											+parseInt(message['adhocCount'])+parseInt(message['regularCount'])
sel-scraper-1  | 											+parseInt(message['gumCount'])+parseInt(message['corporatesCount']));
sel-scraper-1  | 									$('#donateAmount').text(parseInt(message['charitiesCount'])+parseInt(message['campaignCount']));
sel-scraper-1  | 									$('#volunteerAmount').text(parseInt(message['adhocCount'])+parseInt(message['regularCount']));
sel-scraper-1  | 									$('#orgAmount').text(parseInt(message['gumCount'])+parseInt(message['corporatesCount']));
sel-scraper-1  | 								}
sel-scraper-1  | 							}
sel-scraper-1  | 								
sel-scraper-1  | 							if ($('#searchText').val() != undefined && (jQuery.trim( $('#searchText').val() )).length > 0 ) {
sel-scraper-1  | 								$('#searchedText').text("for '" + $('#searchText').val() +"'" );
sel-scraper-1  | 							}else {
sel-scraper-1  | 								$('#searchedText').text("");
sel-scraper-1  | 							}
sel-scraper-1  | 							searchResultHolder.show();
sel-scraper-1  | 							if (message['hasNext']) {
sel-scraper-1  | 								$('.card--loading-flex').css('display', 'block');
sel-scraper-1  | 							} else {
sel-scraper-1  | 								$('.card--loading-flex').css('display', 'none');
sel-scraper-1  | 							}
sel-scraper-1  | 							$('.triggerCampaignDonateNow').bind("click", function(){
sel-scraper-1  | 								triggerDonateNow(true,'22203', $(this).data('id'), 'CAMPAIGN',
sel-scraper-1  | 								60 , null, false, 'search', $(this).data('url'));
sel-scraper-1  | 							});
sel-scraper-1  | 							$('.triggerNPODonateNow').bind("click", function(){
sel-scraper-1  | 								triggerDonateNow(true,'22203', $(this).data('id'), 'NPO ',
sel-scraper-1  | 							60 , null, false, "search", $(this).data('url'));
sel-scraper-1  | 						 	});
sel-scraper-1  | 						}
sel-scraper-1  | 			
sel-scraper-1  | 						} else {
sel-scraper-1  | 							searchListingHolder.empty();
sel-scraper-1  | 							searchResultHolder.find('strong').text(0);
sel-scraper-1  | 							searchResultHolder.show();
sel-scraper-1  | 							loadMore.parent().hide();
sel-scraper-1  | 							loadMore.hide();
sel-scraper-1  | 						}
sel-scraper-1  | 
sel-scraper-1  | 					bindCardClickEvent();
sel-scraper-1  | 				}
sel-scraper-1  | 			});
sel-scraper-1  | 		}
sel-scraper-1  | 	}
sel-scraper-1  | 
sel-scraper-1  | 	function bindCardClickEvent() {
sel-scraper-1  | 		$('.cpn-sgtn-item').off();
sel-scraper-1  | 		$('.cpn-sgtn-item').on('click', function(e) {
sel-scraper-1  | 			window.open($(this).find('h5>a').attr('href'),'_blank');
sel-scraper-1  | 			return false;
sel-scraper-1  | 		});
sel-scraper-1  | 	}
sel-scraper-1  | 	
sel-scraper-1  | 	//find by searchFilter
sel-scraper-1  | 	function getSearchFilter() {
sel-scraper-1  | 		
sel-scraper-1  | 		var filter = {};
sel-scraper-1  | 		count = 0 ;
sel-scraper-1  | 		var keyword  =  $('#searchText').val();
sel-scraper-1  | 		$('#search_term').val(keyword);
sel-scraper-1  | 		// Set keyword
sel-scraper-1  | 		// Set OrgType/Campaign filter
sel-scraper-1  | 		var searchResultType = $('#searchResultType').find('a.is-active').attr("data-target") ;
sel-scraper-1  | 		var targetCategory = [] ;
sel-scraper-1  | 		$("#sortBy").addClass("hide");
sel-scraper-1  | 		if (searchResultType != undefined) {
sel-scraper-1  | 			 $('#searchType').val(searchResultType);
sel-scraper-1  | 		}
sel-scraper-1  | 		var tempTarget = $('#searchType').val();
sel-scraper-1  | 		if ('allType' == tempTarget) {
sel-scraper-1  | 			targetCategory.push('campaign');
sel-scraper-1  | 			targetCategory.push('charities');
sel-scraper-1  | 			targetCategory.push('corporates');
sel-scraper-1  | 			targetCategory.push('groundUp');
sel-scraper-1  | 			targetCategory.push('adHoc');
sel-scraper-1  | 			targetCategory.push('regular');
sel-scraper-1  | 		} else if ('donate' == tempTarget) {
sel-scraper-1  | 			targetCategory.push('charities');
sel-scraper-1  | 			targetCategory.push('campaign');
sel-scraper-1  | 		} else if ('organisation' == tempTarget) {
sel-scraper-1  | 			targetCategory.push('corporates');
sel-scraper-1  | 			targetCategory.push('groundUp');
sel-scraper-1  | 		} else if ('volunteer' == tempTarget) {
sel-scraper-1  | 			targetCategory.push('adHoc');
sel-scraper-1  | 			targetCategory.push('regular');
sel-scraper-1  | 		}
sel-scraper-1  | 		
sel-scraper-1  | 		$('.categoriesFilter').find('.targetCategory').each(function() {
sel-scraper-1  | 			targetCategory = [] ;
sel-scraper-1  | 			targetCategory.push($(this).attr('data-target'));
sel-scraper-1  | 		});
sel-scraper-1  | 		
sel-scraper-1  | 		if (targetCategory != null && targetCategory.length == 1) {
sel-scraper-1  | 			if (targetCategory[0] == "campaign") {
sel-scraper-1  | 				$("#sortBy").removeClass("hide");
sel-scraper-1  | 			}
sel-scraper-1  | 			if(keyword != null && keyword != ""){
sel-scraper-1  | 				$('#sortOrder').find('option[data-alt-label="Relevance"]').removeClass('hide');
sel-scraper-1  | 				
sel-scraper-1  | 			}else{
sel-scraper-1  | 				$('#sortOrder').find('option[data-alt-label="Relevance"]').addClass('hide');
sel-scraper-1  | 				if(!($('#sortOrder').val() == 'campaignEnd' || $('#sortOrder').val() == 'trendingWeight')){
sel-scraper-1  | 					$('#sortOrder').val('campaignEnd');
sel-scraper-1  | 				}
sel-scraper-1  | 			}
sel-scraper-1  | 		}
sel-scraper-1  | 		filter['category'] = targetCategory ;
sel-scraper-1  | 		
sel-scraper-1  |  		var causesList = [] ;
sel-scraper-1  | 		$('.targetCauses').each(function() {
sel-scraper-1  | 			causesList.push($(this).val());
sel-scraper-1  | 		});
sel-scraper-1  | 		
sel-scraper-1  | 		filter['cause'] = causesList ;
sel-scraper-1  | 
sel-scraper-1  | 		var sortOrder = $("option:selected" , $("#sortOrder")).val() ;
sel-scraper-1  | 		filter['sortOrder'] = sortOrder ;
sel-scraper-1  | 
sel-scraper-1  | 		var tdrList = [];
sel-scraper-1  | 		$('#tdrSelection').find('input[type=checkbox]:checked').each(function() {
sel-scraper-1  | 			tdrList.push($(this).val());
sel-scraper-1  | 		});
sel-scraper-1  | 		filter['tdr'] = tdrList;
sel-scraper-1  | 
sel-scraper-1  | 		// Set TDR filter
sel-scraper-1  | 		//typeOfEvent
sel-scraper-1  | 		if (isInArray(targetCategory , 'adHoc') || isInArray(targetCategory , 'regular')   && targetCategory.length <= 2 ) {
sel-scraper-1  | 			filter['typeOfEvent'] = $('input.typeOfEvent:checked').val();
sel-scraper-1  | 		}else{
sel-scraper-1  | 			$('.callSearch.typeOfEvent').prop('checked',false) ; 
sel-scraper-1  | 		}
sel-scraper-1  | 		
sel-scraper-1  | 		//distances/town
sel-scraper-1  | 		if ( ( isInArray(targetCategory , 'adHoc') || isInArray(targetCategory , 'regular')  && targetCategory.length <=2  ) && $('.callSearch.typeOfEvent.physical').is(':checked') ) {
sel-scraper-1  | 			//distances/town
sel-scraper-1  | 			var tabs = $('#locationActiveTabs').val();
sel-scraper-1  | 			if( tabs.indexOf("TOWN") >= 0 ){
sel-scraper-1  | 				var towns = [] ;
sel-scraper-1  | 				towns = $('.townsType').val(); 
sel-scraper-1  | 				filter['town'] = towns ;
sel-scraper-1  | 			
sel-scraper-1  | 			}else if (tabs.indexOf("DISTANCE") >= 0 ){
sel-scraper-1  | 				var targetDistance = $('#distanceList').val();
sel-scraper-1  | 				var postalCode = $('#postalCode').val();
sel-scraper-1  | 				if ((targetDistance != "" && targetDistance != null ) && (postalCode != "" && postalCode != null ) && targetDistance > 0 ) {
sel-scraper-1  | 					filter['distance'] = targetDistance ;
sel-scraper-1  | 					filter['geoLocLatitude'] = $('#geoLocLatitude').val();
sel-scraper-1  | 					filter['geoLocLongitude'] = $('#geoLocLongitude').val();
sel-scraper-1  | 				}
sel-scraper-1  | 			}
sel-scraper-1  | 		}else if( !isInArray(targetCategory , 'adHoc') && !isInArray(targetCategory , 'regular')){
sel-scraper-1  | 			$('#postalCode').val('');
sel-scraper-1  | 			$('.js-town-search').val('');
sel-scraper-1  | 			$(".js-town-search").select2({
sel-scraper-1  | 				  placeholder: "Select a town"
sel-scraper-1  | 				});
sel-scraper-1  | 			$('.select2-selection--multiple').css("padding-top" , "0px");
sel-scraper-1  | 		}
sel-scraper-1  | 		
sel-scraper-1  | 		//wishlist
sel-scraper-1  | 		if (isInArray(targetCategory , 'charities')  && targetCategory.length <= 1 ) {
sel-scraper-1  | 			//wishlist
sel-scraper-1  | 			var wishList = [] ;
sel-scraper-1  | 			$('input[type=checkbox].wishListType:checked').each(function() {
sel-scraper-1  | 				wishList.push($(this).val());
sel-scraper-1  | 			});
sel-scraper-1  | 			filter['wishList'] = wishList ;
sel-scraper-1  | 		}else{
sel-scraper-1  | 			$('input[type=checkbox].wishListType').attr('checked',false) ; 
sel-scraper-1  | 		}
sel-scraper-1  | 		//openings
sel-scraper-1  | 		if ( (isInArray(targetCategory , 'adHoc') || isInArray(targetCategory , 'regular') )  && targetCategory.length <=2 ) {
sel-scraper-1  | 			// openingsType
sel-scraper-1  | 			var allOpen = []
sel-scraper-1  | 			$('input.openingsType:checked').each(function() {
sel-scraper-1  | 				var openings = [] ;
sel-scraper-1  | 				openings.push($(this).attr('min'),$(this).attr('max'));
sel-scraper-1  | 				allOpen.push(openings);
sel-scraper-1  | 			});
sel-scraper-1  | 			filter['opening'] = allOpen ;
sel-scraper-1  | 		}else{
sel-scraper-1  | 			$('input[type=checkbox].openingsType').attr('checked',false) ; 
sel-scraper-1  | 		}
sel-scraper-1  | 		
sel-scraper-1  | 		//accredit status
sel-scraper-1  | 		if (isInArray(targetCategory , 'regular') && targetCategory.length <=1 ) {
sel-scraper-1  | 			// suitabilityType
sel-scraper-1  | 			var accreditStatus = [];
sel-scraper-1  | 			$('input.accreditType:checked').each(function() {
sel-scraper-1  | 				accreditStatus.push($(this).val());
sel-scraper-1  | 			});
