sel-scraper-1  | <script>
sel-scraper-1  | 
sel-scraper-1  | 	$(function(){
sel-scraper-1  | 		$('.setup-org').unbind('click');
sel-scraper-1  | 		$('.setup-org').on('click',function(e){
sel-scraper-1  | 			e.preventDefault();
sel-scraper-1  | 		});
sel-scraper-1  | 		
sel-scraper-1  | 	    $('#uf-dockbar-sidebar a.roleSwitch').unbind('click');
sel-scraper-1  | 	    $('#uf-dockbar-sidebar a.roleSwitch').on('click', function(e){
sel-scraper-1  | 	    	e.preventDefault();   			
sel-scraper-1  |     			switchRoleAjax($(this).attr('data-org'),$(this).attr('data-role'));
sel-scraper-1  |     	    	$('#sidebar a.roleSwitch').removeClass('active');
sel-scraper-1  |     	    	$(this).addClass('active');
sel-scraper-1  | 	    });
sel-scraper-1  | 	    	  
sel-scraper-1  | 	    var switchRoleAjax = (function(){});
sel-scraper-1  | 	    switchRoleAjax = (function(orgId, roleId){
sel-scraper-1  | 	    	var url = Liferay.ThemeDisplay.getPortalURL() + 
sel-scraper-1  | 			'/c/my_account/role_switching' +
sel-scraper-1  | 			'?current_organization='+ orgId +
sel-scraper-1  | 			'&active_role=' + roleId;
sel-scraper-1  | 	    	
sel-scraper-1  | 			var switchRoleAjax = AUI().io.request(url.toString(), {
sel-scraper-1  | 			    method: 'GET',
sel-scraper-1  | 			    dataType: 'json',
sel-scraper-1  | 			    data: {
sel-scraper-1  | 			    	p_auth: Liferay.authToken
sel-scraper-1  | 			    },
sel-scraper-1  | 			    on: {
sel-scraper-1  | 			    	success : function(data) {
sel-scraper-1  | 			    		var response = this.get('responseData');
sel-scraper-1  | 			    		var redirectURL = response.redirectURL;
sel-scraper-1  | 			    		if (redirectURL) {
sel-scraper-1  | 			    			document.location.href = redirectURL;
sel-scraper-1  | 			    		} else {
sel-scraper-1  | 			    			window.location.href = window.location.href;
sel-scraper-1  | 			    		}
sel-scraper-1  | 				    },
sel-scraper-1  | 				    error : function(HttpRequest, textStatus, errorThrown) {
sel-scraper-1  | 				    	console.log(HttpRequest + textStatus + errorThrown);
sel-scraper-1  | 				    }
sel-scraper-1  | 		   	    }
sel-scraper-1  | 			});
sel-scraper-1  | 	    });
sel-scraper-1  | 	    
sel-scraper-1  | 	    var themeSidebar = true;
sel-scraper-1  | 	    var updateWrapperHeight = (function(){});
sel-scraper-1  | 	    updateWrapperHeight = (function(){
sel-scraper-1  | 	    	if (!themeSidebar) {
sel-scraper-1  | 				var wrapperHeight = $('.section-page.dashboard').height();
sel-scraper-1  | 				var sideBarHeight = $('#sidebar ul').height();
sel-scraper-1  | 				var homepageBarHeight = $('.home-page').height();
sel-scraper-1  | 				var navBarHeight = $(".company-profile__head").height();
sel-scraper-1  | 				console.log('sideBarHeight:'+ sideBarHeight + ',wrapperHeight:' + wrapperHeight + ',homepageBarHeight:' +homepageBarHeight);
sel-scraper-1  | 				
sel-scraper-1  | 				if (sideBarHeight > wrapperHeight) {
sel-scraper-1  | 					wrapperHeight = sideBarHeight;
sel-scraper-1  | 				}
sel-scraper-1  | 				console.log((wrapperHeight-homepageBarHeight));
sel-scraper-1  | 				$('.wrapper.dashboard-wrapper').css('min-height', (wrapperHeight-homepageBarHeight-navBarHeight) + 'px');
sel-scraper-1  | 			}
sel-scraper-1  | 	    });
sel-scraper-1  | 	    
sel-scraper-1  | 		$(window).off('updateWrapperHeight', updateWrapperHeight);
sel-scraper-1  |     	$(window).on('updateWrapperHeight', updateWrapperHeight);
sel-scraper-1  |     	$(window).trigger('updateWrapperHeight', updateWrapperHeight);
sel-scraper-1  | 	});
sel-scraper-1  | 	
sel-scraper-1  | 	$(document).ready(function() {
sel-scraper-1  | 		var hiddenOrgId=$("#currentOrgId").attr("data-orgId");		
sel-scraper-1  | 		if(hiddenOrgId!="0"){			
sel-scraper-1  | 			$("#roleSwitch-"+hiddenOrgId).removeClass("hide");
sel-scraper-1  | 			$("#roleSwitch-individual").addClass("hide");
sel-scraper-1  | 		}else{
sel-scraper-1  | 			
sel-scraper-1  | 		}
sel-scraper-1  | 	});
sel-scraper-1  | </script>
